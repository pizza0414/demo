<script type="text/javascript">
var rand1 = Math.floor(Math.random() * 9) + 1; 
document.write(rand1); 
document.write("<b>×</b>");
var rand2 = Math.floor(Math.random() * 9) + 1;
document.write(rand2); 
document.write("<b>＝</b>");
      function check(){
	var ansvalue = document.getElementById("ans").value;
	var rand3 = rand1 * rand2;

	console.log("ans is " + ansvalue);

        if(ansvalue == rand3){
           document.getElementById("die").innerHTML = "ok";
        } else {
           document.getElementById("die").innerHTML = "残念";
        }
      }
    </script>
<input type="text" id="ans" value="">
    <input type="button" onclick="check()" value="next">
 <div id="die">何問とけるかな</div>
